Horner<-function(coeffs,x) {
res <- vector()
for (i in 1:length(x)) {
temp = coeffs[1]
for (j in 2:length(coeffs)) {
temp = temp*x[i] + coeffs[j]
res[i] <- temp
}
}
return(res)
}
